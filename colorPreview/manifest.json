{
    "name": "Color Preview",
    "description": "Preview colors mentioned in text!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.6.0.min.js","colorPreview.js"]
    }]
  }